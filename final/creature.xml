<!-- Example structure for a biped in MuJoCo's XML -->
<mujoco>
    <worldbody>
        <body name="torso" pos="0 0 1">
            <geom shape="box" size="0.1 0.1 0.1" />
            <body name="right_leg" pos="0 0 -0.2">
                <joint name="right_hip" type="hinge" axis="0 1 0" />
                <geom shape="box" size="0.05 0.05 0.2" />
                <body name="right_lower_leg" pos="0 0 -0.4">
                    <joint name="right_knee" type="hinge" axis="0 1 0" />
                    <geom shape="box" size="0.05 0.05 0.2" />
                </body>
            </body>
            <body name="left_leg" pos="0 0 -0.2">
                <!-- Similar structure for the left leg -->
            </body>
        </body>
    </worldbody>
    <!-- Define actuators and sensors as needed -->
</mujoco>
